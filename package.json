{
  "name": "vue-2-breadcrumbs",
  "version": "0.1.2",
  "description": "Breadcrumbs for Vue.js 2.0",
  "license": "MIT",
  "repository": "gitscrum/vue-2-breadcrumbs",
  "homepage": "github.com/gitscrum/vue-2-breadcrumbs#readme",
  "bugs": "gitscrum/vue-2-breadcrumbs/issues",
  "author": {
    "name": "Ivan Demidov",
    "email": "Scrum@list.ru",
    "url": "https://twitter.com/Scrum_"
  },
  "main": "dist/vue-2-breadcrumbs.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "commitmsg": "commitlint --extends=@commitlint/config-angular -e",
    "postpublish": "conventional-changelog -i changelog.md -s -r 0 && git commit -am \"build: update changelog\"",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && babel src/ -d dist/",
    "prepublish": "npm run build",
    "lintjs": "eslint ./src/*.js",
    "lintmd": "eslint --ext md --rule indent: [error, 4] .",
    "lint": "clinton && npm run lintjs && npm run lintmd",
    "test": "npm run lint",
    "update": "updtr"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "breadcrumbs",
    "plugin",
    "vue",
    "vuejs"
  ],
  "devDependencies": {
    "@commitlint/cli": "^5.2.0",
    "@commitlint/config-angular": "^5.1.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-babili": "0.0.12",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "clinton": "^0.13.0",
    "conventional-changelog-cli": "^1.2.0",
    "eslint": "^3.19.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-config-xo": "^0.18.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-markdown": "^1.0.0-beta.2",
    "eslint-plugin-vue": "^2.0.1",
    "eslint-plugin-xo": "^1.0.0",
    "husky": "^0.13.4"
  },
  "browser": "dist/vue-2-breadcrumbs.js",
  "unpkg": "dist/vue-2-breadcrumbs.js",
  "style": "dist/vue-2-breadcrumbs.css",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          },
          "modules": "umd"
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "xo",
      "vue",
      "babel",
      "require-path-exists",
      "markdown"
    ],
    "extends": [
      "vue",
      "xo",
      "plugin:xo/recommended"
    ]
  },
  "clinton": {
    "ignores": [
      "test/**",
      "tmp/**",
      "dist/**",
      "*.{html,jpg}"
    ],
    "rules": {
      "pkg-main": [
        "off"
      ],
      "xo": [
        "off"
      ],
      "ava": [
        "off"
      ],
      "use-travis": [
        "off"
      ]
    }
  }
}
