{
  "name": "vue-2-breadcrumbs",
  "version": "0.0.1",
  "description": "Breadcrumbs for Vue.js",
  "license": "MIT",
  "repository": "gitscrum/vue-breadcrumbs",
  "homepage": "gitscrum/vue-breadcrumbs#readme",
  "bugs": "gitscrum/vue-breadcrumbs/issues",
  "author": {
    "name": "Ivan Demidov",
    "email": "Scrum@list.ru",
    "url": "https://twitter.com/Scrum_"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "commitmsg": "conventional-changelog-lint -e",
    "postpublish": "conventional-changelog -i changelog.md -s -r 0 && git commit -am \"chore(changelog): Update changelog\"",
    "patch": "np patch --any-branch",
    "minor": "np minor --any-branch",
    "major": "np major --any-branch",
    "clean": "rm -rf lib && mkdir lib",
    "build": "npm run clean && babel src/ -d lib/",
    "prepublish": "npm run build",
    "lintjs": "eslint ./src/*.js",
    "lintmd": "eslint --ext md --rule indent: [error, 4] .",
    "lint": "clinton && npm run lintjs && npm run lintmd",
    "test": "npm run lint",
    "update": "updtr"
  },
  "files": [
    "lib/"
  ],
  "keywords": [
    "breadcrumbs",
    "plugin",
    "vue",
    "vuejs"
  ],
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-babili": "0.0.10",
    "babel-preset-env": "^1.1.8",
    "babel-register": "^6.22.0",
    "clinton": "^0.11.0",
    "conventional-changelog-cli": "^1.2.0",
    "conventional-changelog-lint": "^1.1.0",
    "eslint": "^3.14.1",
    "eslint-config-vue": "^2.0.2",
    "eslint-config-xo": "^0.17.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-markdown": "^1.0.0-beta.2",
    "eslint-plugin-require-path-exists": "^1.1.5",
    "eslint-plugin-vue": "^2.0.1",
    "eslint-plugin-xo": "^1.0.0",
    "husky": "^0.13.1",
    "np": "^2.12.0",
    "updtr": "^0.2.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          },
          "modules": "umd"
        }
      ]
    ]
  },
  "eslintConfig": {
    "format": "node_modules/eslint-formatter-pretty",
    "parser": "babel-eslint",
    "plugins": [
      "xo",
      "vue",
      "babel",
      "require-path-exists",
      "markdown"
    ],
    "extends": [
      "vue",
      "xo",
      "plugin:xo/recommended"
    ]
  },
  "clinton": {
    "ignores": [
      "test/**",
      "tmp/**",
      "lib/**",
      "*.{html,jpg}"
    ],
    "rules": {
      "pkg-main": [
        "off"
      ],
      "xo": [
        "off"
      ],
      "ava": [
        "off"
      ],
      "use-travis": [
        "off"
      ]
    }
  }
}
